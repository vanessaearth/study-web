<view class="page">
  <view class="title">{{data.name}}</view>
  <view class="box-one">
    <view wx:for="{{data.detail}}">
      <view>
        {{index+1}}.{{item.name}}
      </view>
      <view class="flex-box">
        <view class="link" wx:for="{{item.list}}" wx:key="index" data-link="{{item.link}}" bindtap="handleOpen">
          {{item.name}}
        </view>
      </view>
    </view>
  </view>
</view>